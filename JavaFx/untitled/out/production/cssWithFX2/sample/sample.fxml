<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.web.WebView?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10"
          stylesheets="@style.css" fx:id="gridPane" >


    <Button text="Open Button" onAction="#handleClick" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <Button text="2" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <Button text="3" GridPane.rowIndex="0" GridPane.columnIndex="2"/>
    <Button text="4" id="button_three" GridPane.rowIndex="0" GridPane.columnIndex="3"/>
    <Button text="5" GridPane.rowIndex="0" GridPane.columnIndex="4"/>
    <Button text="6" GridPane.rowIndex="0" GridPane.columnIndex="5"/>

    <Label text="JavaFX Effects" fx:id="label" GridPane.rowIndex="1"
           GridPane.columnIndex="0"
    onMouseEntered="#handleMouseEnter" onMouseExited="#handleMouseExit"/>
    <Hyperlink text="www.google.com" GridPane.rowIndex="2" GridPane.columnIndex="0" onAction="#handleLink"/>
    <WebView fx:id="webView" GridPane.rowIndex="3" GridPane.columnIndex="0" GridPane.rowSpan="2"/>
</GridPane>